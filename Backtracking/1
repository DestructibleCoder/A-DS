#include <stdio.h>

int c;
void gen(int, int, int, int *);

int main(void) {
  int n = 0, k;
  scanf("%d%d", &k, &n);
  c = k;
  int ans[n] = {};
  gen(0, k, n, ans);
}

void gen(int i, int k, int n, int *ans) {
  if (i == n) {
    for (int x = 0; x < k; x++)
      printf("%d", ans[x]);
    printf("\n");
    return;
  }
  for (int j = 0; j < 2; j++) {
    if (j == 0) {
      if (n - i < c)
        continue;
    } else if (j == 1) {
      if (!c)
        continue;
      else
        c--;
    }
    ans[i] = j;
    gen(i + 1, k, n, ans);
    if (ans[i])
      c++;
  }
}
