#include <algorithm>
#include <iostream>
#include <vector>

using namespace std;

int saw_up(int s, int e, vector<int> k, vector<vector<int>> &dp);

int main() {
  int l, n;
  cin >> l >> n;

  vector<int> k(n + 2);
  k[0] = 0;
  k[n + 1] = l;
  for (int i = 1; i < n + 1; i++)
    cin >> k[i];

  sort(k.begin(), k.end());

  vector<vector<int>> dp(k.size(), vector<int>(k.size(), -1));

  cout << saw_up(0, k.size() - 1, k, dp) << endl;
}

int saw_up(int s, int e, vector<int> k, vector<vector<int>> &dp) {
  if (e - s < 2) {
    dp[s][e] = 0;
    return 0;
  }
  if (e - s == 2) {
    dp[s][e] = k[e] - k[s];
    return k[e] - k[s];
  } else {
    unsigned int min = 0 - 1;
    for (int i = s + 1; i < e; i++) {
      int tmp = (dp[s][i] != -1 ? dp[s][i] : saw_up(s, i, k, dp)) +
                (dp[i][e] != -1 ? dp[i][e] : saw_up(i, e, k, dp));
      if (tmp < min)
        min = tmp;
    }
    return min + k[e] - k[s];
  }
}
